{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [2, "always"],
    "comma-dangle": [2, "always-multiline"],
    "max-len": 0,
    "arrow-parens": 0,
    "babel/arrow-parens": 2,
    "react/jsx-no-bind": 0
  }
}
