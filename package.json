{
  "name": "react-seamstress",
  "version": "0.5.0",
  "repository": "https://github.com/namuol/react-seamstress",
  "description": "Experimental interface for exposing style-related state inside complex React components",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "preinstall": "./install_git_hooks.sh",
    "test": "babel-node test | tap-difflet",
    "hack": "nodemon -w src -w test -w package.json --exec \"npm run build && npm test\"",
    "build": "babel src -d lib",
    "example": "cd examples/simple && npm install && npm start",
    "start": "npm run example",
    "lint": "eslint .",
    "build-docs": "remark . --frail --quiet -o",
    "prepublish": "npm run build"
  },
  "author": "Louis Acresti <louis.acresti@gmail.com> (http://namuol.github.io)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "enzyme": "^2.3.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "nodemon": "^1.7.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.1.0",
    "remark": "^4.2.2",
    "remark-toc": "^3.0.0",
    "remark-validate-links": "^3.0.0",
    "tap": "^1.4.1",
    "tap-difflet": "^0.4.0",
    "tape": "^4.2.1"
  },
  "peerDependencies": {
    "react": ">=0.13.0 <16"
  },
  "keywords": [
    "react-component",
    "style",
    "css",
    "inline",
    "theme",
    "skin"
  ],
  "dependencies": {
    "invariant": "^2.2.0",
    "warning": "^2.1.0"
  }
}
